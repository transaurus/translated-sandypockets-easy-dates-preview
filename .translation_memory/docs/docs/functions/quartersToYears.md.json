      }
    },
    {
      "segment_id": "ac7ef717",
      "source_content": "| prop       | type     | required | default  |\n|------------|----------|----------|----------|\n| `quarters` | `number` | yes      | `null`   |",
      "source_content_hash": "3f8296c9707617740656ccd771cf86b5a85766fdf21a24c0ade009b2b728625c",
      "node_type": "table",
