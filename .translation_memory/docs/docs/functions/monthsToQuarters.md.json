      }
    },
    {
      "segment_id": "ac7ef717",
      "source_content": "| prop     | type     | required | default  |\n|----------|----------|----------|----------|\n| `months` | `number` | yes      | `null`   |",
      "source_content_hash": "f4c0909064c99d0c74f718276e324f6e30204d06b60306a64d7b6911b46cc66c",
      "node_type": "table",
